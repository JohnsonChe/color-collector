{
  "manifest_version": 3,
  "name": "Color Collector",
  "version": "1.0.0",
  "action": {
    "default_icon": "defaultIcon.png",
    "default_popup": "popup.html"
  },
  "description": "Collects all site colors",
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ]
    }
  ]
}